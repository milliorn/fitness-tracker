{
  "name": "fitness-tracker",
  "version": "2.15.0",
  "private": true,
  "scripts": {
    "archive": "tar -czf fitness-tracker-src.tar.gz --exclude=node_modules --exclude=.next --exclude=out --exclude=coverage --exclude=.turbo --exclude=.vercel --exclude=cypress/videos --exclude=cypress/screenshots --exclude=dist --exclude=build --exclude=.DS_Store --exclude=.env --exclude=.env.* --exclude=*.log --exclude=*.sqlite --exclude=*.sqlite3 --exclude=*.db --exclude=*.db-wal --exclude=*.db-shm --exclude=*.tsbuildinfo --exclude=.eslintcache --exclude=.cache --exclude=.git --exclude=fitness-tracker-src.tar.gz .",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit --pretty false",
    "lint:format": "npx --yes prettier@3 --write --ignore-unknown \"**/*\" \"!**/*.md\"",
    "lint": "eslint .",
    "lint:ci": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "npm run typecheck && npm run lint && npm run cy:run:e2e",
    "test:ci": "npm run typecheck && eslint . --max-warnings=0 && cypress run --e2e --browser chrome",
    "test:prebuild": "npm run typecheck && npm run lint -- --max-warnings=0",
    "cy:open:e2e": "cypress open --e2e",
    "cy:run:e2e": "cypress run --e2e",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.8",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.6",
    "better-auth": "^1.4.5",
    "better-sqlite3": "^12.6.2",
    "next": "15.5.11",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/cypress": "^10.1.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axe-core": "^4.11.0",
    "cypress": "^15.5.0",
    "cypress-axe": "^1.7.0",
    "cypress-real-events": "^1.15.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "typescript": "^5"
  }
}